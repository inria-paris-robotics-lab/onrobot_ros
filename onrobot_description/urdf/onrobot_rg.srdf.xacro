        <link name="${prefix}_bracket"/>
        <link name="${prefix}_body"/>
        <link name="${prefix}_finger_1_moment_arm"/>
        <link name="${prefix}_finger_1_finger_tip"/>
        <link name="${prefix}_finger_1_flex_finger"/>
        <link name="${prefix}_finger_1_truss_arm"/>
